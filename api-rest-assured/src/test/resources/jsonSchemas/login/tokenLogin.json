{
  "$id": "tokenLogin",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "token",
    "status"
  ],
  "properties": {
    "token": {
      "title": "Token",
      "type": "string",
      "minLength": 20,
      "maxLength": 400
    },
    "status": {
      "title": "Status message",
      "type": "string",
      "minLength": 4,
      "maxLength": 20
    }
  }
}